{
  "name": "task-management",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc -p tsconfig.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/database/connection/index.ts",
    "schema:drop": "npm run typeorm schema:drop",
    "m:generate": "npm run typeorm migration:generate ./src/database/migrations/%npm_config_name%",
    "m:run": "npm run typeorm migration:run",
    "m:revert": "npm run typeorm migration:revert",
    "m:fresh": "npm run typeorm schema:drop && npm run typeorm migration:run",
    "db:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n ./src/config/ormconfig.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "minio": "7.1.3",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "mysql2": "^3.9.2",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "typeorm-seeding": "1.6.1",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "1.4.11",
    "@types/node": "^20.11.17",
    "nodemon": "^3.0.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}
